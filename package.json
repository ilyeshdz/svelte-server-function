{
	"name": "svelte-server-function",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bunx -b vite dev",
		"build": "bunx -b vite build",
		"preview": "bunx -b vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && bunx -b svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && bunx -b svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"ts-morph": "^26.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vite-plugin-inspect": "^11.1.0"
	}
}
